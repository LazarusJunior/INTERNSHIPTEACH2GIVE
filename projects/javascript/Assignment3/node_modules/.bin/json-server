#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/lib/node_modules:/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/node_modules:/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules:/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/lib/node_modules:/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/node_modules:/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules:/mnt/c/Users/LKJ/Desktop/Teach2Give/projects/javascript/Assignment3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json-server/lib/bin.js" "$@"
else
  exec node  "$basedir/../json-server/lib/bin.js" "$@"
fi
